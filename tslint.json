{
    "extends": "tslint-config-standard",
    "rules": {
        "indent": [true, "spaces", 4],
        "ter-indent": [true, 4],
        "no-eval": false,
        "no-empty": false,
        "no-string-throw": false,
        "space-before-function-paren": false
    },
    "linterOptions": {
        "exclude": ["src/lib/tests", "src/*.spec.ts"]
    }
}
